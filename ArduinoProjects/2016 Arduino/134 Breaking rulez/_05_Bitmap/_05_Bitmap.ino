#define cbi(sfr, bit) (_SFR_BYTE(sfr) &= ~_BV(bit))

void setup() {
  DDRD = B11111111; // Set all bits of PORTD as output

  delay(2000);
  cbi(TIMSK0, TOIE0); // Turn off timer interrupt
}

void loop() {
  DrawBitmap();
}

void DrawBitmap()
{
  int horizUnit = 90;

  PORTD = 250; // Higher than any other point, we'll use this to trigger the scope

  for (uint8_t x = 0; x < xMax; x++)
  {
    for (uint8_t xBit = 0; x < 8; xBit++)
    {
      uint8_t bitAnalogValue = 10;
      for (uint8_t y = 0; y < yMax; y++)
      {
        int theByte = img[x + y*yMax];
        bitAnalogValue += 7;
        if (bitRead(vertByte, 7 - y)) PORTD = bitAnalogValue;// else PORTD = 0;
        delayMicroseconds(horizUnit);
      }
    }
  }
  delayMicroseconds(8000);
}
